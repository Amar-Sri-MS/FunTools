      5 15:00 uint16_t epcq_req_create_intid /* FLAG_INT_EPSQ=0: interrupt MSI/-X vectorid, FLAG_INT_EPSQ=1: interrupt concentrator epcqid */
      6 63:48 uint16_t epcq_req_create_tph_cpuid /* CPUID for TPH */
      6 47:00  uint8_t rsvd3[6]
