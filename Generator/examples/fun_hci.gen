    STRUCT fun_admin_epcq_req_destroy destroy
      2 63:56  uint8_t epcq_req_destroy_subop /* FUN_ADMIN_SUBOP_DESTROY */
      2 55:32  uint8_t rsvd0[3]
      2 31:00 uint32_t epcq_req_destroy_id /* epcqid to destroy */
    END
    STRUCT fun_admin_epcq_req_list list
      2 63:56  uint8_t epcq_req_list_subop /* SUBOP_LIST */
      2 55:48  uint8_t rsvd0
      2 47:32 uint16_t epcq_req_list_flag /* see fun_admin_epcq_create_flag enum */
      2 31:00 uint32_t epcq_req_list_startid /*  */
    END
  END
END

